<Window x:Class="FotoProhlizec.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FotoProhlizec"
        mc:Ignorable="d"
        Title="Prohlížeč obrázků" Height="450" Width="800">
    <Grid>
        <Image x:Name="PhotoImg" Margin="10,10,10,60"/>
        <Button x:Name="LoadBtn" Content="Load" HorizontalAlignment="Left" Margin="10,0,0,10" Width="75" Height="20" VerticalAlignment="Bottom" Click="LoadBtn_Click"/>
    </Grid>
</Window>

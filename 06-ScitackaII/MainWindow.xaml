<Window x:Class="ScitackaII.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScitackaII"
        mc:Ignorable="d"
        Title="MainWindow" Height="194.2" Width="353.839">
    <Grid Margin="0,0,3.8,21">
        <StackPanel HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top" Width="311">
            <TextBox x:Name="Num1" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="311" />
            <TextBox x:Name="Num2" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="311"/>
            <TextBox x:Name="Num3" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="311"/>
            <Button x:Name="CalculateBtn" Content="Sečti" Click="CalculateBtn_Click"/>
            <TextBox x:Name="ResultNum" Height="23" TextWrapping="Wrap" Text="0" IsEnabled="False"/>
        </StackPanel>

    </Grid>
</Window>
